<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>

   <!-- ChangeLog table name and schema -->
   <entry key="CHANGELOG_SCHEMA">SITI</entry>
   <entry key="CHANGELOG_TABLE">DEFRA_CHANGELOG</entry>

   <!-- Driver for database connection -->
   <entry key="database_driver">oracle.jdbc.driver.OracleDriver</entry>

   <!-- URL for database connection -->
   <entry key="database_url">jdbc:oracle:thin:@caporacledb:<%= scope.function_hiera(["oracle-port"]) %>/<%= scope.function_hiera(["capd-oracle-sid"]) %></entry>

   <!-- TNS name given to the service in the tnsnames.ora configuration file -->
	<entry key="database_tns_alias"><%= scope.function_hiera(["capd-oracle-sid"]) %></entry>

	<!-- Password of the SYSTEM user -->
	<entry key="user_system"><%= scope.function_hiera(["capd-oracle-sys-username"]) %></entry>
	<entry key="password_system"><%= scope.function_hiera(["capd-oracle-password-system"]) %></entry>

   <!-- Password of the SITI user -->
	<entry key="password_siti"><%= @abaco_db_siti_password %></entry>

   <!-- Password of the SITIKNOW user -->
	<entry key="password_sitiknow"><%= @abaco_db_sitiknow_password %></entry>

   <!-- Password of the SITIKNOW_APPS user -->
	<entry key="password_sitiknowapps"><%= @abaco_db_sitiknow_apps_password %></entry>

   <!-- Password of the FASCICOLO user -->
	<entry key="password_fascicolo"><%= @abaco_db_fascicolo_password %></entry>

   <!-- Password of the PAMS user -->
	<entry key="password_pams"><%= @abaco_db_pams_password %></entry>

   <!-- Password of the DYLA user -->
	<entry key="password_dyla"><%= @abaco_db_dyla_password %></entry>

   <!-- Password of the SITIIMPO schema -->
   <entry key="password_sitiimpo"><%= scope.function_hiera(["abaco-db-sitiimpo-password"]) %></entry>

   <!-- Configuration of the schema SITI -->
   <entry key="server_ip"><%= @capapp_ip_host %></entry>
   <entry key="server_base_URL">capapp</entry>
   <entry key="log_path">D:/abaco/logs</entry>
   <entry key="temp_path">D:/abaco/temp</entry>
   <entry key="data_path">D:/abaco/data</entry>
   <entry key="alt_RMI_URL">http://192.168.10.12:8998/alternativeRMI13/altrmiservlet</entry>
   <entry key="SMTP_HOST">capmanagement</entry>

   <!-- Configuration of the schema FASCICOLO -->
   <entry key="DWG_path">D:/data/mappe/</entry>
   <entry key="print_path">D:/abaco/data/stampe</entry>
   <entry key="msg_smtp_server">capmanagement</entry>
   <entry key="msg_smtp_password"></entry>
   <entry key="msg_smtp_username"></entry>
   <entry key="process_fide_from"><%= @abaco_smtp_from %></entry>
   <entry key="process_fide_to"><%= @abaco_smtp_to %></entry>
   <entry key="url_riepilogo"><%= @capd_external_web_address %></entry>

   <!-- DbGIS server port -->
   <entry key="server-port-dbgis"><%= @abaco_dbgis_port %></entry>

</properties>
