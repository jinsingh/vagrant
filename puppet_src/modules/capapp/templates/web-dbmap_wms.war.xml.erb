     <url-pattern>/admin/DbMAPManagement</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>wms_agri_view_part</servlet-name>
    <url-pattern>/wms-agri-view-part/WMSservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>wms_agri_view_azie</servlet-name>
    <url-pattern>/wms-agri-view-azie/WMSservlet</url-pattern>
  </servlet-mapping>
