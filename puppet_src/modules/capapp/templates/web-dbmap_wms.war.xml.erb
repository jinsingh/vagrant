      <param-value>http://localhost/siticatasto/actionsNoSSO/SV.dbmapNoSSO/getWmsProject</param-value>
    </init-param>
    <init-param>
      <param-name>dynamicLayerConfiguration</param-name>
      <param-value>true</param-value>
      </init-param>
    <init-param>
      <param-name>SRS</param-name>
      <param-value>none</param-value>
    </init-param>
    <init-param>
      <param-name>direct</param-name>
      <param-value>Y</param-value>
    </init-param>
    <init-param>
      <param-name>jpgQuality</param-name>
      <param-value>0.9</param-value>
