Defaults:deploy !requiretty
Cmnd_Alias DEPLOY_HIERA = /bin/rm -Rf /etc/puppet/hieradata, /bin/cp -Rv hieradata /etc/puppet
Cmnd_Alias YUM_DEPLOY_PUPPET = /bin/rm -Rf /etc/puppet/manifests /etc/puppet/modules /etc/puppet/hiera.yaml
Cmnd_Alias YUM_INSTALL_PUPPET = /usr/bin/yum makecache, /usr/bin/yum update capd-puppet -y, /usr/bin/yum update capd-hieradata-* -y, /usr/bin/yum install capd-hieradata-* -y
Cmnd_Alias RUN_PUPPET = /usr/bin/puppet apply -v --modulepath\=/etc/puppet/modules\:/etc/puppet/vendor/modules --show_diff /etc/puppet/manifests/site.pp
Cmnd_Alias RUN_PUPPET_NO_COLOR = /usr/bin/puppet apply --color\=false --modulepath\=/etc/puppet/modules --show_diff /etc/puppet/manifests/site.pp
Cmnd_Alias DEPLOY_SSUI = /sbin/initctl stop capdssui, /sbin/initctl start capdssui
Cmnd_Alias DEPLOY_SENDMAIL = /sbin/initctl stop capdsendmail, /sbin/initctl start capdsendmail
Cmnd_Alias DEPLOY_MOCKIDAHUB = /sbin/initctl stop capdmockidahub, /sbin/initctl start capdmockidahub
Cmnd_Alias DEPLOY_ABACO_DB = /usr/bin/java -jar db-update-*.jar -upd-dir scripts
Cmnd_Alias YUM_INSTALL_SSUI = /usr/bin/yum update capdssui -y
Cmnd_Alias YUM_INSTALL_MOCKIDAHUB = /usr/bin/yum update capdmocksplayidahub -y
Cmnd_Alias YUM_INSTALL_CAPDINTERNALLOAD = /usr/bin/yum update capdinternalload -y
Cmnd_Alias YUM_INSTALL_SERVICES = /usr/bin/yum update capdservices -y, /usr/bin/yum update capdauth -y
Cmnd_Alias YUM_INSTALL_TASKS = /usr/bin/yum update capdtasks -y
Cmnd_Alias YUM_INSTALL_SENDMAIL = /usr/bin/yum update capdsendmail -y
Cmnd_Alias YUM_INSTALL_MATCHING = /usr/bin/yum update capdmatching -y
Cmnd_Alias YUM_INSTALL_CAPDDATA = /usr/bin/yum update capddata -y
Cmnd_Alias YUM_INSTALL_CAPDAUTHDATA = /usr/bin/yum update capdauthdata -y
Cmnd_Alias YUM_INSTALL_CAPDINTERNALUSERAUTHSERVICES = /usr/bin/yum update capdinternaluserauthservices -y
Cmnd_Alias YUM_INSTALL_CAPDINTERNALUSERACTIVATION = /usr/bin/yum update capdinternaluseractivation -y
Cmnd_Alias YUM_INSTALL_SERVICES_MOCK = /usr/bin/yum update capdsitiserviceintegration -y
Cmnd_Alias YUM_INSTALL_IDA_BETA = /usr/bin/yum update capdidaprivatebetatokensbatch -y
Cmnd_Alias YUM_INSTALL_MDM_DATA_LOAD = /usr/bin/yum update capdmdmload -y
Cmnd_Alias DEPLOY_SERVICES = /sbin/initctl stop capdservices, /sbin/initctl start capdservices, /sbin/initctl start capdauth, /sbin/initctl stop capdauth
Cmnd_Alias DEPLOY_SERVICES_MOCK = /sbin/initctl stop capdsitiserviceintegration, /sbin/initctl start capdsitiserviceintegration
Cmnd_Alias DEPLOY_SITI_AGRI_APP = /sbin/service tomcat6 stop, /bin/cp -fv Tomcat_WAR/*.war /usr/share/tomcat6/webapps/, /sbin/service tomcat6 start, /sbin/service tomcat6 restart
Cmnd_Alias YUM_DEPLOY = /bin/tar -C /var/www/html/yum/x86_64/RPMS -xf -, /bin/rm -Rf /var/www/yum/x86_64/RPMS/capdpuppet*.rpm,  /bin/rm -Rf /var/www/yum/x86_64/RPMS/capdservices*.rpm, /bin/rm -Rf /var/www/yum/x86_64/RPMS/capdssui*.rpm, /bin/rm -Rf /var/www/yum/x86_64/RPMS/capdtasks*.rpm, /bin/rm -Rf /var/www/yum/x86_64/RPMS/capddata*.rpm, /bin/rm -Rf /var/www/yum/x86_64/RPMS/capdsitiserviceintegration*.rpm, /bin/rm -Rf /var/www/html/yum/x86_64/RPMS/capd*, /bin/rm -Rf /var/www/html/yum/x86_64/RPMS/astrium*, /usr/bin/createrepo /var/www/html/yum/x86_64/, /usr/bin/createrepo --update /var/www/html/yum/x86_64/, /usr/bin/createrepo --update --verbose /var/www/html/yum/x86_64/
#Cmnd_Alias ORACLE_WRITE_PERMISSIONS = /usr/bin/setfacl -m u:deploy:rwx <%= @oracle_installation_dir %>
Cmnd_Alias YUM_INSTALL_PLAYAUTH = /usr/bin/yum update capdplayauth -y
Cmnd_Alias YUM_INSTALL_MATCHING_ADAPTER = /usr/bin/yum update capdmatchingadapter -y, /sbin/initctl start capdmatchingadapter, /sbin/initctl stop capdmatchingadapter
Cmnd_Alias YUM_RUN_PLAYAUTH = /sbin/initctl start capdplayauth, /sbin/initctl stop capdplayauth
Cmnd_Alias YUM_MATCHING_DEPLOY = /sbin/initctl start capdmatching, /sbin/initctl stop capdmatching
Cmnd_Alias TRAFFIC_CONTROL = /sbin/tc
Cmnd_Alias LISTENER_CONTROL = /oracle/dbhome_1/bin/lsnrctl
Cmnd_Alias YUM_INSTALL_SITI_TOMCAT_RPM = /usr/bin/yum update capd-siti-tomcat -y
Cmnd_Alias YUM_INSTALL_SITI_SQLPLUS_RPM = /usr/bin/yum update capd-siti-sqlplus -y
Cmnd_Alias YUM_INSTALL_SITI_DELETE_RPM = /usr/bin/yum update capd-siti-delete-db -y
Cmnd_Alias YUM_INSTALL_EXPIRE_PASSWORDS = /usr/bin/yum update capdexpirepasswords -y

deploy ALL = NOPASSWD: DEPLOY_HIERA, YUM_DEPLOY_PUPPET, YUM_INSTALL_PUPPET , RUN_PUPPET, DEPLOY_SSUI, YUM_INSTALL_SSUI, YUM_INSTALL_SERVICES, YUM_INSTALL_CAPDDATA, YUM_INSTALL_CAPDAUTHDATA, YUM_INSTALL_CAPDINTERNALUSERAUTHSERVICES, DEPLOY_SERVICES, YUM_DEPLOY, YUM_INSTALL_SERVICES_MOCK, DEPLOY_SERVICES_MOCK, DEPLOY_SITI_AGRI_APP, ORACLE_WRITE_PERMISSIONS, YUM_INSTALL_TASKS, YUM_INSTALL_PLAYAUTH, YUM_RUN_PLAYAUTH, DEPLOY_ABACO_DB, YUM_INSTALL_MATCHING, YUM_MATCHING_DEPLOY, TRAFFIC_CONTROL, YUM_INSTALL_MOCKIDAHUB, YUM_INSTALL_CAPDINTERNALLOAD, DEPLOY_MOCKIDAHUB, DEPLOY_SENDMAIL, YUM_INSTALL_SENDMAIL, YUM_INSTALL_IDA_BETA, RUN_PUPPET_NO_COLOR, YUM_INSTALL_MATCHING_ADAPTER, YUM_INSTALL_MDM_DATA_LOAD, LISTENER_CONTROL, YUM_INSTALL_SITI_TOMCAT_RPM, YUM_INSTALL_SITI_SQLPLUS_RPM, YUM_INSTALL_SITI_DELETE_RPM, YUM_INSTALL_EXPIRE_PASSWORDS, YUM_INSTALL_CAPDINTERNALUSERACTIVATION
