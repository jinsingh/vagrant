http:
  rootPath: /*

logging:

  console:
     enabled: true

     # Do not display log statements below this threshold to stdout.
     threshold: ALL

     logFormat: '%date{ISO8601} level=[%level] logger=[%logger] thread=[%thread] message=[%message] %replace(exception=[%xException]){''^exception=\[\]$'',''''}%n%nopex'

  file:
     enabled: false

     # Do not write log statements below this threshold to the file.
     threshold: INFO

     currentLogFilename: log/capd-mocks.log

     archive: false

     logFormat: '%date{ISO8601} level=[%level] logger=[%logger] thread=[%thread] message=[%message] %replace(exception=[%xException]){''^exception=\[\]$'',''''}%n%nopex'

sitiAgriClient:
  baseUrl: http://<%= scope.function_hiera(["capapp-ip-host"]) %>:8080
  useMocks: false