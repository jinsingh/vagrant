# Generated by Puppet
LoadPlugin "postgresql"

<Plugin "postgresql">
<% @databases.each do |iname,ihash| -%>
  <Database "<%= iname %>">
<% if ihash['host'] -%>
    Host "<%= ihash['host'] %>"
<% end -%>
<% if ihash['port'] -%>
    Port "<%= ihash['port'] %>"
<% end -%>
<% if ihash['user'] -%>
    User "<%= ihash['user'] %>"
<% end -%>
<% if ihash['password'] -%>
    Password "<%= ihash['password'] %>"
<% end -%>
<% if ihash['interval'] -%>
    Interval "<%= ihash['interval'] %>"
<% end -%>
<% if ihash['instance'] -%>
    Instance "<%= ihash['instance'] %>"
<% end -%>
<% if ihash['service'] -%>
    Service "<%= ihash['service'] %>"
<% end -%>
<% if ihash['krbsrvname'] -%>
    KRBSrvName "<%= ihash['krbsrvname'] %>"
<% end -%>
<% if ihash['sslmode'] -%>
    SSLMode "<%= ihash['sslmode'] %>"
<% end -%>
<% ihash['query'].each do |query| -%>
    Query "<%= query %>"
<% end -%>
  </Database>
<% end -%>
</Plugin>
